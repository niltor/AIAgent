{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Components": {
    // memory/redis/hybrid
    "Cache": "Memory",
    // sqlserver/postgresql
    "Database": "PostgreSQL",
    "UseCors": true,
    // jwt/oauth
    "AuthType": "Jwt",
    // none/rabbitmq/kafka
    "MQType": "None",
    "UseSMS": false,
    "UseSmtp": false,
    "UseAWSS3": false,
    "UseOpenAPI": true
  },
  "Cors": {
    "AllowedOrigins": [],
    "AllowedSubdomains": false
  },
  "Authentication": {
    "Jwt": {
      "ValidAudiences": "AIAgent.AllFreedomPeople.Default",
      "ValidIssuer": "AIAgent.HumanFreedom.Default",
      "Sign": "AIAgent.FightForFreedomForever.Justice",
      "ExpiredSecond": 7200,
      "RefreshExpiredSecond": 604800 //7天
    },
    "Microsoft": {
      "ClientId": null,
      "ClientSecret": null,
      "CallbackUrl": "/your/callback/"
    },
    "Google": {
      "ClientId": null,
      "ClientSecret": null,
      "CallbackUrl": "/your/callback"
    }

  },
  // 缓存配置
  "Cache": {
    // 最大内容字节
    "MaxPayloadBytes": 1048576,
    // 键长度
    "MaxKeyLength": 1024,
    // 过期 minutes
    "Expiration": 20,
    "LocalCacheExpiration": 10
  },
  "Otel": {
    "Exporter": {
      "Otlp": {
        "Endpoint": "https://localhost:4317",
        "Protocol": "grpc",
        "Headers": ""
      }
    },
    "Service": {
      "Name": "AIAgent"
    }
  }
}
